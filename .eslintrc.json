{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": [
        "airbnb-base",
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:playwright/playwright-test"
    ],
    "rules": {
        "no-useless-constructor": "off",
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "no-await-in-loop": "off"
    }
}